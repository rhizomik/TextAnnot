{
  "name": "textannot-client",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build-heroku": "ng build --prod --configuration heroku",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e-local": "npm-run-all e2e-quick e2e-report --continue-on-error",
    "e2e-quick": "ng e2e --dev-server-target= --webdriver-update false",
    "e2e-report": "node e2e/generate-html-report.js",
    "webdriver-update": "webdriver-manager update",
    "extract-i18n": "ng xi18n --i18n-format xlf --output-path i18n --i18n-locale en && xliffmerge --profile xliffmerge.json",
    "start-es": "ng serve --configuration=es"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.5",
    "@angular/cdk": "^6.4.7",
    "@angular/cli": "^6.1.5",
    "@angular/common": "^6.1.5",
    "@angular/compiler": "^6.1.5",
    "@angular/core": "^6.1.5",
    "@angular/forms": "^6.1.5",
    "@angular/http": "^6.1.5",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.5",
    "@angular/platform-browser-dynamic": "^6.1.5",
    "@angular/router": "^6.1.5",
    "@angular/service-worker": "^6.1.5",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@ng-bootstrap/ng-bootstrap": "^3.3.1",
    "@ngx-i18nsupport/tooling": "^1.0.1",
    "angular-tree-component": "^8.0.0",
    "angular4-hal-aot": "^7.8.4",
    "bootstrap": "^4.3.0",
    "core-js": "^2.4.1",
    "cucumber": "4.2.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "ng2-file-upload": "^1.3.0",
    "popper.js": "^1.14.7",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.5",
    "@angular/compiler-cli": "^6.1.5",
    "@angular/language-service": "^6.1.5",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "chai": "^4.1.2",
    "codelyzer": "^4.0.1",
    "cucumber": "^4.1.0",
    "cucumber-html-reporter": "^4.0.2",
